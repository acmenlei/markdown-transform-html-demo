#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Library/coderlei-pro/coderlei-pro-example/vue-pro/md-parser-demo/node_modules/.pnpm/vue-tsc@0.40.13_typescript@4.8.4/node_modules/vue-tsc/bin/node_modules:/Library/coderlei-pro/coderlei-pro-example/vue-pro/md-parser-demo/node_modules/.pnpm/vue-tsc@0.40.13_typescript@4.8.4/node_modules/vue-tsc/node_modules:/Library/coderlei-pro/coderlei-pro-example/vue-pro/md-parser-demo/node_modules/.pnpm/vue-tsc@0.40.13_typescript@4.8.4/node_modules:/Library/coderlei-pro/coderlei-pro-example/vue-pro/md-parser-demo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Library/coderlei-pro/coderlei-pro-example/vue-pro/md-parser-demo/node_modules/.pnpm/vue-tsc@0.40.13_typescript@4.8.4/node_modules/vue-tsc/bin/node_modules:/Library/coderlei-pro/coderlei-pro-example/vue-pro/md-parser-demo/node_modules/.pnpm/vue-tsc@0.40.13_typescript@4.8.4/node_modules/vue-tsc/node_modules:/Library/coderlei-pro/coderlei-pro-example/vue-pro/md-parser-demo/node_modules/.pnpm/vue-tsc@0.40.13_typescript@4.8.4/node_modules:/Library/coderlei-pro/coderlei-pro-example/vue-pro/md-parser-demo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vue-tsc.js" "$@"
else
  exec node  "$basedir/../../bin/vue-tsc.js" "$@"
fi
